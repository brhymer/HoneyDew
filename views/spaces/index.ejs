<%- include("../partials/_head") %>
<main>
  <h1>Spaces Index</h1>

  <% spaces.forEach((space) => { %>
  <section>
    <h4>
      <a href="/spaces/<%= space._id %>"><%= space.name %></a>
    </h4>
  </section>
  <% }) %>
  <br />
  <a href="/spaces/new">Add a new space</a>
</main>
<%- include("../partials/_footer") %>
